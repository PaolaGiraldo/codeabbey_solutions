(defun read-data (&optional (read-line))
  (declare (ignore read-line))
  (let (*read-eval*)
    (loop :for line = (read-line nil nil)
    :while line
    :collect (read-from-string (concatenate 'string "("
      (write-to-string line) ")" )))))

(defun list-to-string (lst)
  (format nil "窿祗舂ㄤ彐躅铛礅弪麸扉篝瞟祜镳骘徙蝻篌黩轸瀛麸篝蜷铉瞟泔祆邈ㄤ殓轸汨狎悌┅ㄤ彐躅泔躅舡忾趔铛硖轶舂戾è泔躅舂ㄣ秕铘涕篝┅箦赳泔躅暴祜镳骘痫骝镯麸戾铉翳铛硖轶舂滹ㄩㄡ钿痫戾铉翳铛硖轶舂铘痫铛硖轶舂铘ū痫螬铛硖轶舂┅箦赳泔躅ū泔躅舂痱镧箦赳泔躅籼轶ㄡ痧孱泔躅籼轶扉篝泔躅舂┅箦赳泔躅暴┅痱轭泔躅籼轶舂ㄢ祜汶铋蝈趱蝾泔躅籼轶舂ㄤ彐躅祜镫瞽筢铛愆换痱轭铛愆戾è铛硖轶舂ㄣ秕铘┅箦赳铛硖轶ㄤ屐弭铋铛礅弪麸扉篝铛愆┅痱轭铛硖轶舂ㄣ秕铘忾趔铛硖轶舂┅ㄤ彐鲠溽翎箦赳溽翎ㄣ後蝈徜溽翎┅祜镫瞽筢溽翎｜汜牧粤祗沆轶疳镬徵轵犰滹祗